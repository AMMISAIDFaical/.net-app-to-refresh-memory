{
  "name": ".NET in Codespaces",
  "image": "mcr.microsoft.com/dotnet/sdk:10.0",
  "features": {
    "ghcr.io/devcontainers/features/dotnet:2": {
      "version": "none",
      "dotnetRuntimeVersions": "10.0"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csdevkit"
      ]
    }
  },
  "remoteEnv": {
    "DOTNET_MULTILEVEL_LOOKUP": "0",
    "TARGET": "net10.0"
  }
}
